// Detect It Easy: detection rule file format

init("packer", "PeX");

function detect(bShowType, bShowVersion, bShowOptions) {
    if (PE.compareEP("E9$$$$$$$$60e8$$$$$$$$83c404e8")) {
        sVersion = "0.99";
        bDetected = true;
    }

    return result(bShowType, bShowVersion, bShowOptions);
}